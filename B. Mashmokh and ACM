#include <bits/stdc++.h>
using namespace std;

int tab[3000][3000];
int n,k;

int solve(int pos,int tam){
	
	if(tab[pos][tam] != -1) return tab[pos][tam];
	
	if(tam==k) return tab[pos][tam]=1;
	
	int resp = 0;

	for(int i=pos;i<=n;i++){
		
		if(i%pos==0){
			
			resp = ((resp%1000000007)+(solve(i,tam+1)%1000000007))%1000000007;
			
		}
		
	}
	
	return tab[pos][tam] = resp;
	
}

int main(){
	
	cin >> n >> k;

	memset(tab,-1,sizeof(tab));
	
	int soma = 0;
	
	for(int i=1;i<=n;i++){
		
		soma = ((soma%1000000007)+(solve(i,1))%1000000007)%1000000007;
		
	}
	
	cout << soma << endl;
		
	return 0;
}
